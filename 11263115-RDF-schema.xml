<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xml:base="http://www.example.org/ontology">

    <rdfs:Class rdf:ID="Classroom">
        <rdfs:subClassOf rdf:resource="#Room"/>
    </rdfs:Class>
    <rdfs:Class rdf:ID="Student"/>

    <rdf:Property rdf:ID="hasStudent">
        <rdfs:domain rdf:resource="#Classroom"/>
        <rdfs:range rdf:resource="#Student"/>
    </rdf:Property>
    <rdf:Property rdf:ID="hasComputer">
        <rdfs:domain rdf:resource="#Classroom"/>
        <rdfs:range rdf:resource="#Computer"/>
    </rdf:Property>
    <rdf:Property rdf:ID="hasScreen">
        <rdfs:domain rdf:resource="#Classroom"/>
        <rdfs:range rdf:resource="#Screen"/>
    </rdf:Property>

    <rdfs:Class rdf:ID="Computer"/>
    <rdfs:Class rdf:ID="Screen"/>

</rdf:RDF>
